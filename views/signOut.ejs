<!doctype html>
<html>
<head>
    <title>Log Out</title>
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.2.1/jquery.min.js"></script>
    <link href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-BVYiiSIFeK1dGmJRAkycuHAHRg32OmUcww7on3RYdg4Va+PmSTsz/K68vbdEjh4u" crossorigin="anonymous"> <!-- load bootstrap css -->
    <link href="https://maxcdn.bootstrapcdn.com/font-awesome/4.7.0/css/font-awesome.min.css" rel="stylesheet" integrity="sha384-wvfXpqpZZVQGK6TAh5PVlGOfQNHSoD2xbE+QkPxCAFlNEevoEH3Sl0sibVcOQVnN" crossorigin="anonymous"> <!-- load fontawesome -->
     <link href="https://fonts.googleapis.com/css?family=Crete+Round|Crimson+Text|Libre+Baskerville" rel="stylesheet">
    <link rel="stylesheet" type="text/css" href="css/grades/index.css">
    
</head>
<body>
<div class="container">
  <div class="row">
    <div class="col-sm-4"></div>
      <div class="col-sm-4">
        <img class="profile-img" src="/css/img/tr.png"><br><br>
      </div>
    <div class="col-sm-4"></div>
  </div>
      <div class="row">
        <div class="col-sm-4"></div>
          <div class="col-sm-4">
            <h3 class="text-center" style="margin-left: 30px" > Sign in with </h3><br>
              <div class="text-center">
                 <a href="/auth/google" id="google" class="btn btn-danger"><span class="fa fa-google-plus"></span> Google</a>
              </div>
          </div>
        <div class="col-sm-4"></div>
      </div>
</div>
<!-- END LOGIN -->
<script>

function checkUser() {
  $.get('/user').done(function(response) {
    console.log(response);
    if (response) {
       window.location.replace("/home")
       return true;
    } else{  
       return false;
    }
  });
}
checkUser();
</script>
</body>
</html>



